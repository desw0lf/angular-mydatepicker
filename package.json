{
  "name": "angular-mydatepicker-example",
  "version": "0.13.0",
  "description": "Angular datepicker",
  "keywords": [
    "angular",
    "date",
    "picker",
    "datepicker",
    "range"
  ],
  "author": "nodro7",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodro7/angular-mydatepicker.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-lib": "node scripts/delete-dist-folder.js && ng build AngularMyDatePicker --configuration production && node scripts/copy-artifacts.js",
    "build-example": "ng build --base-href=\"https://nodro7.github.io/angular-mydatepicker/\"",
    "test-lib": "node scripts/delete-test-output-folder.js && ng test --code-coverage=true AngularMyDatePicker",
    "lint-lib": "ng lint AngularMyDatePicker",
    "pub": "cd dist/legacy-angular-mydatepicker && npm publish --access=public"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.5",
    "@angular/common": "^15.1.5",
    "@angular/compiler": "^15.1.5",
    "@angular/core": "^15.1.5",
    "@angular/forms": "^15.1.5",
    "@angular/platform-browser": "^15.1.5",
    "@angular/platform-browser-dynamic": "^15.1.5",
    "@angular/router": "^15.1.5",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.6",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.1.6",
    "@angular/compiler-cli": "^15.1.5",
    "@angular/language-service": "^15.1.5",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "fs-extra": "^8.0.1",
    "jasmine-core": "^3.10.1",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^15.1.2",
    "typescript": "~4.9.5"
  }
