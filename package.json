{
  "name": "angular-mydatepicker-example",
  "version": "0.11.5",
  "description": "forked - Angular datepicker and date range picker",
  "homepage": "https://kekeh.github.io/angular-mydatepicker/",
  "keywords": [
    "Angular",
    "datepicker",
    "date range picker"
  ],
  "author": "kekeh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kekeh/angular-mydatepicker.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-lib": "node scripts/delete-dist-folder.js && ng build AngularMyDatePicker --configuration=production && node scripts/copy-artifacts.js",
    "build-all": "node scripts/delete-dist-folder.js && ng build AngularMyDatePicker && node scripts/copy-artifacts.js && ng build",
    "test-lib": "node scripts/delete-test-output-folder.js && ng test --code-coverage=true AngularMyDatePicker",
    "lint-lib": "ng lint AngularMyDatePicker",
    "pub": "cd dist/legacy-angular-mydatepicker && npm publish --access=public",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.13",
    "@angular/common": "~12.2.13",
    "@angular/compiler": "~12.2.13",
    "@angular/core": "~12.2.13",
    "@angular/forms": "~12.2.13",
    "@angular/platform-browser": "~12.2.13",
    "@angular/platform-browser-dynamic": "~12.2.13",
    "@angular/router": "~12.2.13",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.13",
    "@angular/cli": "~12.2.13",
    "@angular/compiler-cli": "~12.2.13",
    "@angular/language-service": "~12.2.13",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codecov": "^3.5.0",
    "codelyzer": "^6.0.0",
    "fs-extra": "^8.0.1",
    "jasmine-core": "^3.10.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.2.5",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
